{% if menu %}
	<ul class="display_items">
	{% for item in menu %}
		<li id="display_item" class="{{item.classes | join(' ')}}">
			<a class="menu_target_link" target="{{ item.target }}" href="{{item.get_link}}">{{item.title}}</a>
			{% include "components/menu.twig" with {'menu': item.get_children} %}
		</li>
	{% endfor %}
	</ul>
{% endif %}
