{% extends "./layout/base.twig" %}

{% block content %}
<h1>hello</h1>
 	{% for post in events %}
		<div class="content-wrapper">
			<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
				<section class="article-content">
					<h1 class="article-h1">{{post.title}}</h1>
					<div class="article-body">
						{{post.content}}
					</div>
						<div class="article-body">
						{{post.lieux}}
					</div>
				</section>
			</article>
		</div>
	{% endfor %}	
{% endblock %}

